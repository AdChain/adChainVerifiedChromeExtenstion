{
  "background": {
    "page": "background.html"
  },

  "browser_action": {
    "default_icon": {
      "19": "adtoken.png",
      "38": "adtoken.png"
    },
    "default_title": "adChain Verify",
    "default_popup": "popup.html"
  },
  "description": "Verification with the adChain Registry",
  "content_security_policy": "script-src https://adchain-registry-api.metax.io; object-src 'self'",

  "icons": {
    "48": "adtoken.png",
    "96": "adtoken.png"
  },

  "manifest_version": 2,

  "name": "adChain Verified",

  "permissions": ["webNavigation","notifications","tabs"],

  "version": "0.3",

  "web_accessible_resources": ["adtoken.png"]
}